<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Cache Configuration Settings -->
		<!-- Strategy options: "time" or "size" -->
		<add key="CacheStrategy" value="time"/>

		<!-- Time-based eviction: Cache entries older than this value (in minutes) are removed -->
		<add key="CacheMaxAge" value="60"/>

		<!-- Size-based eviction: Maximum number of cache entries allowed -->
		<add key="CacheMaxElements" value="100"/>
	</appSettings>
</configuration>